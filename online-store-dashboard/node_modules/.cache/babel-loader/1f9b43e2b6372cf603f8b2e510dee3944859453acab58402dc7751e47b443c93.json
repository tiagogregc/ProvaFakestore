{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport { mapState } from 'vuex';\nimport ProductTable from '../components/ProductTable.vue';\nimport Modal from '../components/Modal.vue'; // Supondo que o Modal já esteja configurado\n\nexport default {\n  components: {\n    ProductTable,\n    Modal\n  },\n  data() {\n    return {\n      products: [],\n      filteredProducts: [],\n      categoryFilter: '',\n      sortOption: 'price',\n      showModal: false,\n      selectedProduct: null\n    };\n  },\n  computed: {\n    ...mapState({\n      allProducts: state => state.products // Puxando produtos do Vuex\n    })\n  },\n  mounted() {\n    this.fetchProducts();\n  },\n  methods: {\n    fetchProducts() {\n      // Aqui, você pode fazer a requisição ou acessar os dados no Vuex\n      this.products = this.allProducts;\n      this.filteredProducts = this.products;\n    },\n    applyFilters() {\n      this.filteredProducts = this.products.filter(product => {\n        if (this.categoryFilter && product.category !== this.categoryFilter) {\n          return false;\n        }\n        return true;\n      });\n      this.applySorting(); // Aplica a ordenação após o filtro\n    },\n    applySorting() {\n      if (this.sortOption === 'price') {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOption === 'rating') {\n        this.filteredProducts.sort((a, b) => b.rating.rate - a.rating.rate);\n      }\n    },\n    showProductDetails(product) {\n      this.selectedProduct = product;\n      this.showModal = true;\n    },\n    addProduct() {\n      // Lógica para adicionar produto (abre formulário ou interage com API)\n    },\n    removeProduct() {\n      // Lógica para remover o produto (API ou Vuex)\n      if (this.selectedProduct) {\n        this.products = this.products.filter(product => product.id !== this.selectedProduct.id);\n        this.selectedProduct = null;\n      }\n    },\n    editProduct() {\n      // Lógica para editar o produto\n      if (this.selectedProduct) {\n        // Exemplo: Navegar para página de edição ou abrir um modal de edição\n        this.$router.push(`/edit-product/${this.selectedProduct.id}`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapState","ProductTable","Modal","components","data","products","filteredProducts","categoryFilter","sortOption","showModal","selectedProduct","computed","allProducts","state","mounted","fetchProducts","methods","applyFilters","filter","product","category","applySorting","sort","a","b","price","rating","rate","showProductDetails","addProduct","removeProduct","id","editProduct","$router","push"],"sources":["C:\\Users\\tiago\\WebstormProjects\\FakestoreS\\online-store-dashboard\\src\\views\\ProductList.vue"],"sourcesContent":["<!-- src/views/ProductList.vue -->\r\n<template>\r\n  <div class=\"container\">\r\n    <h2>Produtos</h2>\r\n\r\n    <!-- Filtros e Classificação -->\r\n    <div class=\"filters\">\r\n      <select v-model=\"categoryFilter\" @change=\"applyFilters\">\r\n        <option value=\"\">Selecione a Categoria</option>\r\n        <option value=\"electronics\">Eletrônicos</option>\r\n        <option value=\"jewelery\">Jóias</option>\r\n        <option value=\"men's clothing\">Roupas Masculinas</option>\r\n        <option value=\"women's clothing\">Roupas Femininas</option>\r\n      </select>\r\n\r\n      <select v-model=\"sortOption\" @change=\"applySorting\">\r\n        <option value=\"price\">Preço</option>\r\n        <option value=\"rating\">Classificação</option>\r\n      </select>\r\n    </div>\r\n\r\n    <ProductTable :products=\"filteredProducts\" @viewProduct=\"showProductDetails\" />\r\n\r\n    <!-- Modal para exibir detalhes do produto -->\r\n    <Modal v-if=\"showModal\" @close=\"showModal = false\">\r\n      <div v-if=\"selectedProduct\">\r\n        <h3>{{ selectedProduct.title }}</h3>\r\n        <p>{{ selectedProduct.description }}</p>\r\n        <p><strong>Preço:</strong> {{ selectedProduct.price }}</p>\r\n        <p><strong>Categoria:</strong> {{ selectedProduct.category }}</p>\r\n        <p><strong>Classificação:</strong> {{ selectedProduct.rating.rate }}</p>\r\n      </div>\r\n    </Modal>\r\n\r\n    <!-- Botões para Adicionar, Atualizar e Remover produtos -->\r\n    <div class=\"product-actions\">\r\n      <button @click=\"addProduct\">Adicionar Produto</button>\r\n      <button @click=\"removeProduct\" v-if=\"selectedProduct\">Remover Produto</button>\r\n      <button @click=\"editProduct\" v-if=\"selectedProduct\">Editar Produto</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex';\r\nimport ProductTable from '../components/ProductTable.vue';\r\nimport Modal from '../components/Modal.vue'; // Supondo que o Modal já esteja configurado\r\n\r\nexport default {\r\n  components: { ProductTable, Modal },\r\n  data() {\r\n    return {\r\n      products: [],\r\n      filteredProducts: [],\r\n      categoryFilter: '',\r\n      sortOption: 'price',\r\n      showModal: false,\r\n      selectedProduct: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      allProducts: (state) => state.products, // Puxando produtos do Vuex\r\n    }),\r\n  },\r\n  mounted() {\r\n    this.fetchProducts();\r\n  },\r\n  methods: {\r\n    fetchProducts() {\r\n      // Aqui, você pode fazer a requisição ou acessar os dados no Vuex\r\n      this.products = this.allProducts;\r\n      this.filteredProducts = this.products;\r\n    },\r\n    applyFilters() {\r\n      this.filteredProducts = this.products.filter(product => {\r\n        if (this.categoryFilter && product.category !== this.categoryFilter) {\r\n          return false;\r\n        }\r\n        return true;\r\n      });\r\n      this.applySorting(); // Aplica a ordenação após o filtro\r\n    },\r\n    applySorting() {\r\n      if (this.sortOption === 'price') {\r\n        this.filteredProducts.sort((a, b) => a.price - b.price);\r\n      } else if (this.sortOption === 'rating') {\r\n        this.filteredProducts.sort((a, b) => b.rating.rate - a.rating.rate);\r\n      }\r\n    },\r\n    showProductDetails(product) {\r\n      this.selectedProduct = product;\r\n      this.showModal = true;\r\n    },\r\n    addProduct() {\r\n      // Lógica para adicionar produto (abre formulário ou interage com API)\r\n    },\r\n    removeProduct() {\r\n      // Lógica para remover o produto (API ou Vuex)\r\n      if (this.selectedProduct) {\r\n        this.products = this.products.filter(product => product.id !== this.selectedProduct.id);\r\n        this.selectedProduct = null;\r\n      }\r\n    },\r\n    editProduct() {\r\n      // Lógica para editar o produto\r\n      if (this.selectedProduct) {\r\n        // Exemplo: Navegar para página de edição ou abrir um modal de edição\r\n        this.$router.push(`/edit-product/${this.selectedProduct.id}`);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.filters {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.filters select {\r\n  margin-right: 10px;\r\n}\r\n\r\n.product-actions {\r\n  margin-top: 20px;\r\n}\r\n</style>"],"mappings":";;;AA4CA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,YAAW,MAAO,gCAAgC;AACzD,OAAOC,KAAI,MAAO,yBAAyB,EAAE;;AAE7C,eAAe;EACbC,UAAU,EAAE;IAAEF,YAAY;IAAEC;EAAM,CAAC;EACnCE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,EAAE;MACpBC,cAAc,EAAE,EAAE;MAClBC,UAAU,EAAE,OAAO;MACnBC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGX,QAAQ,CAAC;MACVY,WAAW,EAAGC,KAAK,IAAKA,KAAK,CAACR,QAAQ,CAAE;IAC1C,CAAC;EACH,CAAC;EACDS,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACPD,aAAaA,CAAA,EAAG;MACd;MACA,IAAI,CAACV,QAAO,GAAI,IAAI,CAACO,WAAW;MAChC,IAAI,CAACN,gBAAe,GAAI,IAAI,CAACD,QAAQ;IACvC,CAAC;IACDY,YAAYA,CAAA,EAAG;MACb,IAAI,CAACX,gBAAe,GAAI,IAAI,CAACD,QAAQ,CAACa,MAAM,CAACC,OAAM,IAAK;QACtD,IAAI,I