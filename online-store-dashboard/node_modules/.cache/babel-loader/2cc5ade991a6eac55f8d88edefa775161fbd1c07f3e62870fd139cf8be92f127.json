{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      products: [],\n      // Lista completa de produtos\n      filteredProducts: [],\n      // Lista filtrada\n      productForm: {\n        id: null,\n        title: \"\",\n        category: \"\",\n        price: 0,\n        rating: {\n          rate: 0\n        }\n      },\n      categoryFilter: \"\",\n      sortOption: \"price\",\n      editingProduct: false\n    };\n  },\n  methods: {\n    fetchProducts() {\n      this.products = this.$store.getters.getAllProducts;\n      this.filteredProducts = [...this.products];\n    },\n    applyFilters() {\n      this.filteredProducts = this.products.filter(product => {\n        return !this.categoryFilter || product.category === this.categoryFilter;\n      });\n      this.applySorting();\n    },\n    applySorting() {\n      if (this.sortOption === \"price\") {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOption === \"rating\") {\n        this.filteredProducts.sort((a, b) => b.rating.rate - a.rating.rate);\n      }\n    },\n    deleteProduct(productId) {\n      this.$store.dispatch(\"removeProduct\", productId);\n      this.fetchProducts();\n    },\n    editProduct(product) {\n      this.productForm = {\n        ...product\n      };\n      this.editingProduct = true;\n    },\n    saveProduct() {\n      if (this.editingProduct) {\n        this.$store.dispatch(\"updateProduct\", this.productForm);\n      } else {\n        const newProduct = {\n          ...this.productForm\n        };\n        newProduct.id = Date.now(); // Gera um ID único\n        this.$store.dispatch(\"addProduct\", newProduct);\n      }\n      this.cancelEditing();\n      this.fetchProducts();\n    },\n    cancelEditing() {\n      this.productForm = {\n        id: null,\n        title: \"\",\n        category: \"\",\n        price: 0,\n        rating: {\n          rate: 0\n        }\n      };\n      this.editingProduct = false;\n    }\n  },\n  mounted() {\n    this.fetchProducts();\n  }\n};","map":{"version":3,"names":["data","products","filteredProducts","productForm","id","title","category","price","rating","rate","categoryFilter","sortOption","editingProduct","methods","fetchProducts","$store","getters","getAllProducts","applyFilters","filter","product","applySorting","sort","a","b","deleteProduct","productId","dispatch","editProduct","saveProduct","newProduct","Date","now","cancelEditing","mounted"],"sources":["C:\\Users\\tiago\\WebstormProjects\\FakestoreS\\online-store-dashboard\\src\\views\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>Gestão de Produtos</h2>\r\n\r\n    <!-- Filtros -->\r\n    <div class=\"filters\">\r\n      <select v-model=\"categoryFilter\" @change=\"applyFilters\">\r\n        <option value=\"\">Todas as Categorias</option>\r\n        <option value=\"electronics\">Eletrônicos</option>\r\n        <option value=\"jewelery\">Jóias</option>\r\n        <option value=\"men's clothing\">Roupas Masculinas</option>\r\n        <option value=\"women's clothing\">Roupas Femininas</option>\r\n      </select>\r\n\r\n      <select v-model=\"sortOption\" @change=\"applySorting\">\r\n        <option value=\"price\">Ordenar por Preço</option>\r\n        <option value=\"rating\">Ordenar por Classificação</option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- Tabela de Produtos -->\r\n    <table>\r\n      <thead>\r\n      <tr>\r\n        <th>Nome</th>\r\n        <th>Categoria</th>\r\n        <th>Preço</th>\r\n        <th>Classificação</th>\r\n        <th>Ações</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"product in filteredProducts\" :key=\"product.id\">\r\n        <td>{{ product.title }}</td>\r\n        <td>{{ product.category }}</td>\r\n        <td>R$ {{ product.price.toFixed(2) }}</td>\r\n        <td>{{ product.rating.rate }}</td>\r\n        <td>\r\n          <button @click=\"editProduct(product)\">Editar</button>\r\n          <button @click=\"deleteProduct(product.id)\">Excluir</button>\r\n        </td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Formulário de Adicionar/Editar Produtos -->\r\n    <div class=\"form-container\">\r\n      <h3 v-if=\"editingProduct\">Editar Produto</h3>\r\n      <h3 v-else>Adicionar Produto</h3>\r\n\r\n      <form @submit.prevent=\"saveProduct\">\r\n        <input\r\n            type=\"text\"\r\n            v-model=\"productForm.title\"\r\n            placeholder=\"Nome do Produto\"\r\n            required\r\n        />\r\n\r\n        <select v-model=\"productForm.category\" required>\r\n          <option value=\"\">Selecione uma Categoria</option>\r\n          <option value=\"electronics\">Eletrônicos</option>\r\n          <option value=\"jewelery\">Jóias</option>\r\n          <option value=\"men's clothing\">Roupas Masculinas</option>\r\n          <option value=\"women's clothing\">Roupas Femininas</option>\r\n        </select>\r\n\r\n        <input\r\n            type=\"number\"\r\n            v-model.number=\"productForm.price\"\r\n            placeholder=\"Preço\"\r\n            required\r\n        />\r\n\r\n        <input\r\n            type=\"number\"\r\n            v-model.number=\"productForm.rating.rate\"\r\n            placeholder=\"Classificação (1-5)\"\r\n            min=\"1\"\r\n            max=\"5\"\r\n            step=\"0.1\"\r\n            required\r\n        />\r\n\r\n        <button type=\"submit\">\r\n          {{ editingProduct ? \"Atualizar Produto\" : \"Adicionar Produto\" }}\r\n        </button>\r\n        <button type=\"button\" @click=\"cancelEditing\">Cancelar</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      products: [], // Lista completa de produtos\r\n      filteredProducts: [], // Lista filtrada\r\n      productForm: {\r\n        id: null,\r\n        title: \"\",\r\n        category: \"\",\r\n        price: 0,\r\n        rating: {\r\n          rate: 0,\r\n        },\r\n      },\r\n      categoryFilter: \"\",\r\n      sortOption: \"price\",\r\n      editingProduct: false,\r\n    };\r\n  },\r\n  methods: {\r\n    fetchProducts() {\r\n      this.products = this.$store.getters.getAllProducts;\r\n      this.filteredProducts = [...this.products];\r\n    },\r\n    applyFilters() {\r\n      this.filteredProducts = this.products.filter((product) => {\r\n        return (\r\n            !this.categoryFilter ||\r\n            product.category === this.categoryFilter\r\n        );\r\n      });\r\n      this.applySorting();\r\n    },\r\n    applySorting() {\r\n      if (this.sortOption === \"price\") {\r\n        this.filteredProducts.sort((a, b) => a.price - b.price);\r\n      } else if (this.sortOption === \"rating\") {\r\n        this.filteredProducts.sort((a, b) => b.rating.rate - a.rating.rate);\r\n      }\r\n    },\r\n    deleteProduct(productId) {\r\n      this.$store.dispatch(\"removeProduct\", productId);\r\n      this.fetchProducts();\r\n    },\r\n    editProduct(product) {\r\n      this.productForm = { ...product };\r\n      this.editingProduct = true;\r\n    },\r\n    saveProduct() {\r\n      if (this.editingProduct) {\r\n        this.$store.dispatch(\"updateProduct\", this.productForm);\r\n      } else {\r\n        const newProduct = { ...this.productForm };\r\n        newProduct.id = Date.now(); // Gera um ID único\r\n        this.$store.dispatch(\"addProduct\", newProduct);\r\n      }\r\n      this.cancelEditing();\r\n      this.fetchProducts();\r\n    },\r\n    cancelEditing() {\r\n      this.productForm = {\r\n        id: null,\r\n        title: \"\",\r\n        category: \"\",\r\n        price: 0,\r\n        rating: {\r\n          rate: 0,\r\n        },\r\n      };\r\n      this.editingProduct = false;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchProducts();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.filters {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.filters select {\r\n  margin-right: 10px;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n}\r\n\r\nth, td {\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  text-align: left;\r\n}\r\n\r\nth {\r\n  background-color: #f4f4f4;\r\n}\r\n\r\n.form-container {\r\n  margin-top: 20px;\r\n}\r\n\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\nform input,\r\nform select {\r\n  margin-bottom: 10px;\r\n  padding: 8px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\nform button {\r\n  margin-top: 10px;\r\n  padding: 10px;\r\n  background-color: #28a745;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\nform button[type=\"button\"] {\r\n  background-color: #dc3545;\r\n}\r\n</style>"],"mappings":";;AA6FA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MAAE;MACdC,gBAAgB,EAAE,EAAE;MAAE;MACtBC,WAAW,EAAE;QACXC,EAAE,EAAE,IAAI;QACRC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,CAAC;QACRC,MAAM,EAAE;UACNC,IAAI,EAAE;QACR;MACF,CAAC;MACDC,cAAc,EAAE,EAAE;MAClBC,UAAU,EAAE,OAAO;MACnBC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,IAAI,CAACb,QAAO,GAAI,IAAI,CAACc,MAAM,CAACC,OAAO,CAACC,cAAc;MAClD,IAAI,CAACf,gBAAe,GAAI,CAAC,GAAG,IAAI,CAACD,QAAQ,CAAC;IAC5C,CAAC;IACDiB,YAAYA,CAAA,EAAG;MACb,IAAI,CAAChB,gBAAe,GAAI,IAAI,CAACD,QAAQ,CAACkB,MAAM,CAAEC,OAAO,IAAK;QACxD,OACI,CAAC,IAAI,CAACV,cAAa,IACnBU,OAAO,CAACd,QAAO,KAAM,IAAI,CAACI,cAAa;MAE7C,CAAC,CAAC;MACF,IAAI,CAACW,YAAY,CAAC,CAAC;IACrB,CAAC;IACDA,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACV,UAAS,KAAM,OAAO,EAAE;QAC/B,IAAI,CAACT,gBAAgB,CAACoB,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAAChB,KAAI,GAAIiB,CAAC,CAACjB,KAAK,CAAC;MACzD,OAAO,IAAI,IAAI,CAACI,UAAS,KAAM,QAAQ,EAAE;QACvC,IAAI,CAACT,gBAAgB,CAACoB,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAAChB,MAAM,CAACC,IAAG,GA