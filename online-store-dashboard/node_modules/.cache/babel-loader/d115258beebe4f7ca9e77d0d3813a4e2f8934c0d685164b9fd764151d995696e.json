.newProduct.category && this.newProduct.price && this.newProduct.rating) {\n        // Adiciona o novo produto ao Vuex\n        this.$store.commit('addProduct', this.newProduct);\n\n        // Limpar o formulário após adicionar\n        this.newProduct = {\n          title: '',\n          category: '',\n          price: null,\n          rating: null\n        };\n        this.showAddProductForm = false;\n      } else {\n        alert('Por favor, preencha todos os campos!');\n      }\n    },\n    removeProduct() {\n      if (this.selectedProduct) {\n        this.products = this.products.filter(product => product.id !== this.selectedProduct.id);\n        this.selectedProduct = null;\n      }\n    },\n    editProduct() {\n      if (this.selectedProduct) {\n        this.$router.push(`/edit-product/${this.selectedProduct.id}`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapState","ProductTable","Modal","components","data","products","filteredProducts","categoryFilter","sortOption","showModal","selectedProduct","showAddProductForm","newProduct","title","category","price","rating","computed","allProducts","state","mounted","fetchProducts","methods","applyFilters","filter","product","applySorting","sort","a","b","showProductDetails","toggleAddProductForm","addProduct","$store","commit","alert","removeProduct","id","editProduct","$router","push"],"sources":["C:\\Users\\tiago\\WebstormProjects\\FakestoreS\\online-store-dashboard\\src\\views\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>Produtos</h2>\r\n\r\n    <!-- Filtros e Classificação -->\r\n    <div class=\"filters\">\r\n      <select v-model=\"categoryFilter\" @change=\"applyFilters\">\r\n        <option value=\"\">Selecione a Categoria</option>\r\n        <option value=\"electronics\">Eletrônicos</option>\r\n        <option value=\"jewelery\">Jóias</option>\r\n        <option value=\"men's clothing\">Roupas Masculinas</option>\r\n        <option value=\"women's clothing\">Roupas Femininas</option>\r\n      </select>\r\n\r\n      <select v-model=\"sortOption\" @change=\"applySorting\">\r\n        <option value=\"price\">Preço</option>\r\n        <option value=\"rating\">Classificação</option>\r\n      </select>\r\n    </div>\r\n\r\n    <ProductTable :products=\"filteredProducts\" @viewProduct=\"showProductDetails\" />\r\n\r\n    <!-- Modal para exibir detalhes do produto -->\r\n    <Modal v-if=\"showModal\" @close=\"showModal = false\">\r\n      <div v-if=\"selectedProduct\">\r\n        <h3>{{ selectedProduct.title }}</h3>\r\n        <p>{{ selectedProduct.description }}</p>\r\n        <p><strong>Preço:</strong> {{ selectedProduct.price }}</p>\r\n        <p><strong>Categoria:</strong> {{ selectedProduct.category }}</p>\r\n        <p><strong>Classificação:</strong> {{ selectedProduct.rating.rate }}</p>\r\n      </div>\r\n    </Modal>\r\n\r\n    <!-- Botões para Adicionar, Atualizar e Remover produtos -->\r\n    <div class=\"product-actions\">\r\n      <button @click=\"toggleAddProductForm\">Adicionar Produto</button>\r\n      <button @click=\"removeProduct\" v-if=\"selectedProduct\">Remover Produto</button>\r\n      <button @click=\"editProduct\" v-if=\"selectedProduct\">Editar Produto</button>\r\n    </div>\r\n\r\n    <!-- Formulário para adicionar novo produto -->\r\n    <div v-if=\"showAddProductForm\" class=\"add-product-form\">\r\n      <h3>Adicionar Produto</h3>\r\n      <form @submit.prevent=\"addProduct\">\r\n        <div>\r\n          <label for=\"title\">Nome</label>\r\n          <input v-model=\"newProduct.title\" type=\"text\" id=\"title\" required />\r\n        </div>\r\n        <div>\r\n          <label for=\"category\">Categoria</label>\r\n          <select v-model=\"newProduct.category\" id=\"category\" required>\r\n            <option value=\"\">Selecione uma Categoria</option>\r\n            <option value=\"electronics\">Eletrônicos</option>\r\n            <option value=\"jewelery\">Jóias</option>\r\n            <option value=\"men's clothing\">Roupas Masculinas</option>\r\n            <option value=\"women's clothing\">Roupas Femininas</option>\r\n          </select>\r\n        </div>\r\n        <div>\r\n          <label for=\"price\">Preço</label>\r\n          <input v-model=\"newProduct.price\" type=\"number\" id=\"price\" required />\r\n        </div>\r\n        <div>\r\n          <label for=\"rating\">Classificação</label>\r\n          <input v-model=\"newProduct.rating\" type=\"number\" id=\"rating\" min=\"1\" max=\"5\" required />\r\n        </div>\r\n        <button type=\"submit\">Salvar Produto</button>\r\n        <button type=\"button\" @click=\"toggleAddProductForm\">Cancelar</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex';\r\nimport ProductTable from '../components/ProductTable.vue';\r\nimport Modal from '../components/ModalMode.vue'; // Supondo que o Modal já esteja configurado\r\n\r\nexport default {\r\n  components: { ProductTable, Modal },\r\n  data() {\r\n    return {\r\n      products: [],\r\n      filteredProducts: [],\r\n      categoryFilter: '',\r\n      sortOption: 'price',\r\n      showModal: false,\r\n      selectedProduct: null,\r\n      showAddProductForm: false, // Controle para exibir o formulário de adição de produto\r\n      newProduct: {\r\n        title: '',\r\n        category: '',\r\n        price: null,\r\n        rating: null,\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      allProducts: (state) => state.products, // Puxando produtos do Vuex\r\n    }),\r\n  },\r\n  mounted() {\r\n    this.fetchProducts();\r\n  },\r\n  methods: {\r\n    fetchProducts() {\r\n      this.products = this.allProducts;\r\n      this.filteredProducts = this.products;\r\n    },\r\n    applyFilters() {\r\n      this.filteredProducts = this.products.filter(product => {\r\n        if (this.categoryFilter && product.category !== this.categoryFilter) {\r\n          return false;\r\n        }\r\n        return true;\r\n      });\r\n      this.applySorting(); // Aplica a ordenação após o filtro\r\n    },\r\n    applySorting() {\r\n      if (this.sortOption === 'price') {\r\n        this.filteredProducts.sort((a, b) => a.price - b.price);\r\n      } else if (this.sortOption === 'rating') {\r\n        this.filteredProducts.sort((a, b) => b.rating - a.rating);\r\n      }\r\n    },\r\n    showProductDetails(product) {\r\n      this.selectedProduct = product;\r\n      this.showModal = true;\r\n    },\r\n    toggleAddProductForm() {\r\n      this.showAddProductForm = !this.showAddProductForm;\r\n    },\r\n    addProduct() {\r\n      if (this.newProduct.title && this.newProduct.category && this.newProduct.price && this.newProduct.rating) {\r\n        // Adiciona o novo produto ao Vuex\r\n        this.$store.commit('addProduct', this.newProduct);\r\n\r\n        // Limpar o formulário após adicionar\r\n        this.newProduct = {\r\n          title: '',\r\n          category: '',\r\n          price: null,\r\n          rating: null,\r\n        };\r\n        this.showAddProductForm = false;\r\n      } else {\r\n        alert('Por favor, preencha todos os campos!');\r\n      }\r\n    },\r\n    removeProduct() {\r\n      if (this.selectedProduct) {\r\n        this.products = this.products.filter(product => product.id !== this.selectedProduct.id);\r\n        this.selectedProduct = null;\r\n      }\r\n    },\r\n    editProduct() {\r\n      if (this.selectedProduct) {\r\n        this.$router.push(`/edit-product/${this.selectedProduct.id}`);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.filters {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.filters select {\r\n  margin-right: 10px;\r\n}\r\n\r\n.product-actions {\r\n  margin-top: 20px;\r\n}\r\n\r\n.add-product-form {\r\n  margin-top: 20px;\r\n}\r\n\r\n.add-product-form form {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.add-product-form label {\r\n  margin-bott