{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport { mapState } from 'vuex';\nexport default {\n  data() {\n    return {\n      categoryFilter: '',\n      sortOption: 'price',\n      isFormVisible: false,\n      isEdit: false,\n      formProduct: {\n        id: null,\n        title: '',\n        category: '',\n        price: null,\n        rating: {\n          rate: null\n        }\n      }\n    };\n  },\n  computed: {\n    ...mapState(['products']),\n    filteredProducts() {\n      let filtered = [...this.products];\n\n      // Filtra por categoria\n      if (this.categoryFilter) {\n        filtered = filtered.filter(product => product.category === this.categoryFilter);\n      }\n\n      // Ordena os produtos\n      if (this.sortOption === 'price') {\n        filtered.sort((a, b) => a.price - b.price);\n      } else if (this.sortOption === 'rating') {\n        filtered.sort((a, b) => b.rating.rate - a.rating.rate);\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    openAddForm() {\n      this.isFormVisible = true;\n      this.isEdit = false;\n      this.resetForm();\n    },\n    editProduct(product) {\n      this.isFormVisible = true;\n      this.isEdit = true;\n      this.formProduct = {\n        ...product\n      }; // Cria uma cópia do produto selecionado\n    },\n    deleteProduct(productId) {\n      this.$store.dispatch('removeProduct', productId);\n    },\n    submitForm() {\n      if (this.isEdit) {\n        this.$store.dispatch('updateProduct', this.formProduct);\n      } else {\n        this.formProduct.id = Date.now(); // Gera um ID único para o novo produto\n        this.$store.dispatch('addProduct', this.formProduct);\n      }\n      this.cancelForm();\n    },\n    cancelForm() {\n      this.isFormVisible = false;\n      this.resetForm();\n    },\n    resetForm() {\n      this.formProduct = {\n        id: null,\n        title: '',\n        category: '',\n        price: null,\n        rating: {\n          rate: null\n        }\n      };\n    }\n  }\n};","map":{"version":3,"names":["mapState","data","categoryFilter","sortOption","isFormVisible","isEdit","formProduct","id","title","category","price","rating","rate","computed","filteredProducts","filtered","products","filter","product","sort","a","b","methods","openAddForm","resetForm","editProduct","deleteProduct","productId","$store","dispatch","submitForm","Date","now","cancelForm"],"sources":["C:\\Users\\tiago\\WebstormProjects\\FakestoreS\\online-store-dashboard\\src\\views\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>Gerenciamento de Produtos</h2>\r\n\r\n    <!-- Filtros -->\r\n    <div class=\"filters\">\r\n      <label for=\"categoryFilter\">Filtrar por Categoria:</label>\r\n      <select v-model=\"categoryFilter\" id=\"categoryFilter\" @change=\"applyFilters\">\r\n        <option value=\"\">Todas</option>\r\n        <option value=\"electronics\">Eletrônicos</option>\r\n        <option value=\"jewelery\">Joias</option>\r\n        <option value=\"men's clothing\">Roupas Masculinas</option>\r\n        <option value=\"women's clothing\">Roupas Femininas</option>\r\n      </select>\r\n\r\n      <label for=\"sortOption\">Ordenar por:</label>\r\n      <select v-model=\"sortOption\" id=\"sortOption\" @change=\"applySorting\">\r\n        <option value=\"price\">Preço</option>\r\n        <option value=\"rating\">Classificação</option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- Lista de Produtos -->\r\n    <table class=\"product-table\">\r\n      <thead>\r\n      <tr>\r\n        <th>ID</th>\r\n        <th>Nome</th>\r\n        <th>Categoria</th>\r\n        <th>Preço</th>\r\n        <th>Classificação</th>\r\n        <th>Ações</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"product in filteredProducts\" :key=\"product.id\">\r\n        <td>{{ product.id }}</td>\r\n        <td>{{ product.title }}</td>\r\n        <td>{{ product.category }}</td>\r\n        <td>R$ {{ product.price.toFixed(2) }}</td>\r\n        <td>{{ product.rating.rate }}</td>\r\n        <td>\r\n          <button @click=\"editProduct(product)\">Editar</button>\r\n          <button @click=\"deleteProduct(product.id)\">Excluir</button>\r\n        </td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Formulário de Adicionar/Editar Produto -->\r\n    <div v-if=\"isFormVisible\" class=\"product-form\">\r\n      <h3>{{ isEdit ? 'Editar Produto' : 'Adicionar Produto' }}</h3>\r\n      <form @submit.prevent=\"submitForm\">\r\n        <label for=\"title\">Nome do Produto:</label>\r\n        <input type=\"text\" v-model=\"formProduct.title\" id=\"title\" required />\r\n\r\n        <label for=\"category\">Categoria:</label>\r\n        <input type=\"text\" v-model=\"formProduct.category\" id=\"category\" required />\r\n\r\n        <label for=\"price\">Preço:</label>\r\n        <input type=\"number\" v-model=\"formProduct.price\" id=\"price\" step=\"0.01\" required />\r\n\r\n        <label for=\"rating\">Classificação:</label>\r\n        <input type=\"number\" v-model=\"formProduct.rating.rate\" id=\"rating\" min=\"0\" max=\"5\" step=\"0.1\" required />\r\n\r\n        <button type=\"submit\">{{ isEdit ? 'Atualizar Produto' : 'Adicionar Produto' }}</button>\r\n        <button type=\"button\" @click=\"cancelForm\">Cancelar</button>\r\n      </form>\r\n    </div>\r\n\r\n    <!-- Botão para abrir o formulário de adicionar produto -->\r\n    <button v-if=\"!isFormVisible\" @click=\"openAddForm\">Adicionar Novo Produto</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      categoryFilter: '',\r\n      sortOption: 'price',\r\n      isFormVisible: false,\r\n      isEdit: false,\r\n      formProduct: {\r\n        id: null,\r\n        title: '',\r\n        category: '',\r\n        price: null,\r\n        rating: {\r\n          rate: null,\r\n        },\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(['products']),\r\n    filteredProducts() {\r\n      let filtered = [...this.products];\r\n\r\n      // Filtra por categoria\r\n      if (this.categoryFilter) {\r\n        filtered = filtered.filter(product => product.category === this.categoryFilter);\r\n      }\r\n\r\n      // Ordena os produtos\r\n      if (this.sortOption === 'price') {\r\n        filtered.sort((a, b) => a.price - b.price);\r\n      } else if (this.sortOption === 'rating') {\r\n        filtered.sort((a, b) => b.rating.rate - a.rating.rate);\r\n      }\r\n\r\n      return filtered;\r\n    },\r\n  },\r\n  methods: {\r\n    openAddForm() {\r\n      this.isFormVisible = true;\r\n      this.isEdit = false;\r\n      this.resetForm();\r\n    },\r\n    editProduct(product) {\r\n      this.isFormVisible = true;\r\n      this.isEdit = true;\r\n      this.formProduct = { ...product }; // Cria uma cópia do produto selecionado\r\n    },\r\n    deleteProduct(productId) {\r\n      this.$store.dispatch('removeProduct', productId);\r\n    },\r\n    submitForm() {\r\n      if (this.isEdit) {\r\n        this.$store.dispatch('updateProduct', this.formProduct);\r\n      } else {\r\n        this.formProduct.id = Date.now(); // Gera um ID único para o novo produto\r\n        this.$store.dispatch('addProduct', this.formProduct);\r\n      }\r\n      this.cancelForm();\r\n    },\r\n    cancelForm() {\r\n      this.isFormVisible = false;\r\n      this.resetForm();\r\n    },\r\n    resetForm() {\r\n      this.formProduct = {\r\n        id: null,\r\n        title: '',\r\n        category: '',\r\n        price: null,\r\n        rating: {\r\n          rate: null,\r\n        },\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  max-width: 800px;\r\n  margin: auto;\r\n}\r\n\r\n.filters {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.product-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.product-table th, .product-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\n.product-table th {\r\n  background-color: #f2f2f2;\r\n}\r\n\r\n.product-form {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.product-form label {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.product-form input {\r\n  width: 100%;\r\n  padding: 8px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.product-form button {\r\n  margin-right: 10px;\r\n}\r\n\r\nbutton {\r\n  padding: 8px 16px;\r\n  cursor: pointer;\r\n}\r\n</style>"],"mappings":";;AA4EA,SAASA,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,EAAE;MAClBC,UAAU,EAAE,OAAO;MACnBC,aAAa,EAAE,KAAK;MACpBC,MAAM,EAAE,KAAK;MACbC,WAAW,EAAE;QACXC,EAAE,EAAE,IAAI;QACRC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,IAAI;QACXC,MAAM,EAAE;UACNC,IAAI,EAAE;QACR;MACF;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGb,QAAQ,CAAC,CAAC,UAAU,CAAC,CAAC;IACzBc,gBAAgBA,CAAA,EAAG;MACjB,IAAIC,QAAO,GAAI,CAAC,GAAG,IAAI,CAACC,QAAQ,CAAC;;MAEjC;MACA,IAAI,IAAI,CAACd,cAAc,EAAE;QACvBa,QAAO,GAAIA,QAAQ,CAACE,MAAM,CAACC,OAAM,IAAKA,OAAO,CAACT,QAAO,KAAM,IAAI,CAACP,cAAc,CAAC;MACjF;;MAEA;MACA,IAAI,IAAI,CAACC,UAAS,KAAM,OAAO,EAAE;QAC/BY,QAAQ,CAACI,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACV,KAAI,GAAIW,CAAC,CAACX,KAAK,CAAC;MAC5C,OAAO,IAAI,IAAI,CAACP,UAAS,KAAM,QAAQ,EAAE;QACvCY,QAAQ,CAACI,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACV,MAAM,CAACC,IAAG,GAAIQ,CAAC,CAACT,MAAM,CAACC,IAAI,CAAC;MACxD;MAEA,OAAOG,QAAQ;IACjB;EACF,CAAC;EACDO,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACnB,aAAY,GAAI,IAAI;MACzB,IAAI,CAACC,MAAK,GAAI,KAAK;MACnB,IAAI,CAACmB,SAAS,CAAC,CAAC;IAClB,CAAC;IACDC,WAAWA,CAACP,OAAO,EAAE;MACnB,IAAI,CAACd,aAAY,GAAI,IAAI;MACzB,IAAI,CAACC,MAAK,GAAI,IAAI;MAClB,IAAI,CAACC,WAAU,GAAI;QAAE,GAAGY;MAAQ,CAAC,EAAE;IACrC,CAAC;IACDQ,aAAaA,CAACC,SAAS,EAAE;MACvB,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,eAAe,EAAEF,SAAS,CAAC;IAClD,CAAC;IACDG,UAAUA,CAAA,EAAG;MACX,IAAI,IAA