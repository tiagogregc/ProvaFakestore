{"ast":null,"code":"import UserTable from '@/components/UserTable.vue';\nimport Modal from '@/components/ModalMode.vue'; // Componente ModalMode para adicionar/editar usuário\n\nexport default {\n  components: {\n    UserTable,\n    Modal\n  },\n  data() {\n    return {\n      selectedUser: null,\n      showModal: false,\n      showEditModal: false,\n      isEditMode: false,\n      userForm: {\n        id: null,\n        name: '',\n        email: '',\n        city: ''\n      }\n    };\n  },\n  computed: {\n    // Acessando o estado global 'users' da Vuex Store\n    users() {\n      return this.$store.state.users;\n    }\n  },\n  methods: {\n    showUserDetails(user) {\n      this.selectedUser = user;\n      this.showModal = true;\n    },\n    showCreateUserForm() {\n      this.isEditMode = false;\n      this.userForm = {\n        id: null,\n        name: '',\n        email: '',\n        city: ''\n      };\n      this.showEditModal = true;\n    },\n    editUser(user) {\n      this.isEditMode = true;\n      this.userForm = {\n        ...user\n      }; // Preenche o formulário com os dados do usuário para edição\n      this.showEditModal = true;\n    },\n    deleteUser(userId) {\n      this.$store.dispatch('removeUser', userId); // Chama a ação para deletar o usuário\n    },\n    submitUserForm() {\n      if (this.isEditMode) {\n        // Atualiza o usuário\n        this.$store.dispatch('updateUser', this.userForm);\n      } else {\n        // Adiciona um novo usuário\n        this.$store.dispatch('addUser', {\n          ...this.userForm,\n          id: Date.now()\n        });\n      }\n      this.showEditModal = false; // Fecha o modal após a submissão\n    }\n  }\n};","map":{"version":3,"names":["UserTable","Modal","components","data","selectedUser","showModal","showEditModal","isEditMode","userForm","id","name","email","city","computed","users","$store","state","methods","showUserDetails","user","showCreateUserForm","editUser","deleteUser","userId","dispatch","submitUserForm","Date","now"],"sources":["C:\\Users\\tiago\\WebstormProjects\\FakestoreS\\online-store-dashboard\\src\\views\\UserList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>Usuários</h2>\r\n    <UserTable\r\n        :users=\"users\"\r\n        @viewUser=\"showUserDetails\"\r\n        @editUser=\"editUser\"\r\n        @deleteUser=\"deleteUser\"\r\n    />\r\n\r\n    <!-- Modal para exibir detalhes do usuário -->\r\n    <Modal v-if=\"showModal\" @close=\"showModal = false\">\r\n      <div v-if=\"selectedUser\">\r\n        <h3>{{ selectedUser.name }}</h3>\r\n        <p>Email: {{ selectedUser.email }}</p>\r\n        <p>Cidade: {{ selectedUser.city }}</p>\r\n      </div>\r\n    </Modal>\r\n\r\n    <!-- Modal para adicionar/editar usuário -->\r\n    <Modal v-if=\"showEditModal\" @close=\"showEditModal = false\">\r\n      <div>\r\n        <h3>{{ isEditMode ? 'Editar' : 'Adicionar' }} Usuário</h3>\r\n        <form @submit.prevent=\"submitUserForm\">\r\n          <input v-model=\"userForm.name\" placeholder=\"Nome\" required />\r\n          <input v-model=\"userForm.email\" placeholder=\"Email\" type=\"email\" required />\r\n          <input v-model=\"userForm.city\" placeholder=\"Cidade\" required />\r\n          <button type=\"submit\">{{ isEditMode ? 'Atualizar' : 'Adicionar' }}</button>\r\n        </form>\r\n      </div>\r\n    </Modal>\r\n\r\n    <button @click=\"showCreateUserForm\">Adicionar Usuário</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserTable from '@/components/UserTable.vue';\r\nimport Modal from '@/components/ModalMode.vue'; // Componente ModalMode para adicionar/editar usuário\r\n\r\nexport default {\r\n  components: {\r\n    UserTable,\r\n    Modal,\r\n  },\r\n  data() {\r\n    return {\r\n      selectedUser: null,\r\n      showModal: false,\r\n      showEditModal: false,\r\n      isEditMode: false,\r\n      userForm: {\r\n        id: null,\r\n        name: '',\r\n        email: '',\r\n        city: '',\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    // Acessando o estado global 'users' da Vuex Store\r\n    users() {\r\n      return this.$store.state.users;\r\n    },\r\n  },\r\n  methods: {\r\n    showUserDetails(user) {\r\n      this.selectedUser = user;\r\n      this.showModal = true;\r\n    },\r\n    showCreateUserForm() {\r\n      this.isEditMode = false;\r\n      this.userForm = { id: null, name: '', email: '', city: '' };\r\n      this.showEditModal = true;\r\n    },\r\n    editUser(user) {\r\n      this.isEditMode = true;\r\n      this.userForm = { ...user }; // Preenche o formulário com os dados do usuário para edição\r\n      this.showEditModal = true;\r\n    },\r\n    deleteUser(userId) {\r\n      this.$store.dispatch('removeUser', userId); // Chama a ação para deletar o usuário\r\n    },\r\n    submitUserForm() {\r\n      if (this.isEditMode) {\r\n        // Atualiza o usuário\r\n        this.$store.dispatch('updateUser', this.userForm);\r\n      } else {\r\n        // Adiciona um novo usuário\r\n        this.$store.dispatch('addUser', { ...this.userForm, id: Date.now() });\r\n      }\r\n      this.showEditModal = false; // Fecha o modal após a submissão\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAqCA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,KAAI,MAAO,4BAA4B,EAAE;;AAEhD,eAAe;EACbC,UAAU,EAAE;IACVF,SAAS;IACTC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI;MAClBC,SAAS,EAAE,KAAK;MAChBC,aAAa,EAAE,KAAK;MACpBC,UAAU,EAAE,KAAK;MACjBC,QAAQ,EAAE;QACRC,EAAE,EAAE,IAAI;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE;MACR;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR;IACAC,KAAKA,CAAA,EAAG;MACN,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,KAAK;IAChC;EACF,CAAC;EACDG,OAAO,EAAE;IACPC,eAAeA,CAACC,IAAI,EAAE;MACpB,IAAI,CAACf,YAAW,GAAIe,IAAI;MACxB,IAAI,CAACd,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDe,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACb,UAAS,GAAI,KAAK;MACvB,IAAI,CAACC,QAAO,GAAI;QAAEC,EAAE,EAAE,IAAI;QAAEC,IAAI,EAAE,EAAE;QAAEC,KAAK,EAAE,EAAE;QAAEC,IAAI,EAAE;MAAG,CAAC