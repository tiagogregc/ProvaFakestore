{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport { mapState } from 'vuex';\nimport ProductTable from '../components/ProductTable.vue';\nimport Modal from '../components/Modal.vue'; // Supondo que o Modal já esteja configurado\n\nexport default {\n  components: {\n    ProductTable,\n    Modal\n  },\n  data() {\n    return {\n      products: [],\n      filteredProducts: [],\n      categoryFilter: '',\n      sortOption: 'price',\n      showModal: false,\n      selectedProduct: null,\n      showAddProductForm: false,\n      // Controle para exibir o formulário de adição de produto\n      showEditProductForm: false,\n      // Controle para exibir o formulário de edição de produto\n      newProduct: {\n        title: '',\n        category: '',\n        price: null,\n        rating: null\n      }\n    };\n  },\n  computed: {\n    ...mapState({\n      allProducts: state => state.products // Puxando produtos do Vuex\n    })\n  },\n  mounted() {\n    this.fetchProducts();\n  },\n  methods: {\n    fetchProducts() {\n      this.products = this.allProducts;\n      this.filteredProducts = this.products;\n    },\n    applyFilters() {\n      this.filteredProducts = this.products.filter(product => {\n        if (this.categoryFilter && product.category !== this.categoryFilter) {\n          return false;\n        }\n        return true;\n      });\n      this.applySorting(); // Aplica a ordenação após o filtro\n    },\n    applySorting() {\n      if (this.sortOption === 'price') {\n        this.filteredProducts.sort((a, b) => a.price - b.price);\n      } else if (this.sortOption === 'rating') {\n        this.filteredProducts.sort((a, b) => b.rating - a.rating);\n      }\n    },\n    toggleAddProductForm() {\n      this.showAddProductForm = !this.showAddProductForm;\n    },\n    addProduct() {\n      if (this.newProduct.title && this.newProduct.category && this.newProduct.price && this.newProduct.rating) {\n        // Adiciona o novo produto ao Vuex\n        this.$store.commit('addProduct', this.newProduct);\n\n        // Limpar o formulário após adicionar\n        this.newProduct = {\n          title: '',\n          category: '',\n          price: null,\n          rating: null\n        };\n        this.showAddProductForm = false;\n      } else {\n        alert('Por favor, preencha todos os campos!');\n      }\n    },\n    deleteProduct(product) {\n      this.products = this.products.filter(p => p.id !== product.id);\n    },\n    editProduct(product) {\n      this.selectedProduct = {\n        ...product\n      };\n      this.showEditProductForm = true;\n    },\n    updateProduct() {\n      // Atualiza o produto no Vuex\n      this.$store.commit('updateProduct', this.selectedProduct);\n      this.showEditProductForm = false;\n    },\n    cancelEdit() {\n      this.showEditProductForm = false;\n    }\n  }\n};","map":{"version":3,"names":["mapState","ProductTable","Modal","components","data","products","filteredProducts","categoryFilter","sortOption","showModal","selectedProduct","showAddProductForm","showEditProductForm","newProduct","title","category","price","rating","computed","allProducts","state","mounted","fetchProducts","methods","applyFilters","filter","product","applySorting","sort","a","b","toggleAddProductForm","addProduct","$store","commit","alert","deleteProduct","p","id","editProduct","updateProduct","cancelEdit"],"sources":["C:\\Users\\tiago\\WebstormProjects\\FakestoreS\\online-store-dashboard\\src\\views\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>Produtos</h2>\r\n\r\n    <!-- Filtros e Classificação -->\r\n    <div class=\"filters\">\r\n      <select v-model=\"categoryFilter\" @change=\"applyFilters\">\r\n        <option value=\"\">Selecione a Categoria</option>\r\n        <option value=\"electronics\">Eletrônicos</option>\r\n        <option value=\"jewelery\">Jóias</option>\r\n        <option value=\"men's clothing\">Roupas Masculinas</option>\r\n        <option value=\"women's clothing\">Roupas Femininas</option>\r\n      </select>\r\n\r\n      <select v-model=\"sortOption\" @change=\"applySorting\">\r\n        <option value=\"price\">Preço</option>\r\n        <option value=\"rating\">Classificação</option>\r\n      </select>\r\n    </div>\r\n\r\n    <ProductTable\r\n        :products=\"filteredProducts\"\r\n        @editProduct=\"editProduct\"\r\n        @deleteProduct=\"deleteProduct\"\r\n    />\r\n\r\n    <!-- Modal para exibir detalhes do produto -->\r\n    <Modal v-if=\"showModal\" @close=\"showModal = false\">\r\n      <div v-if=\"selectedProduct\">\r\n        <h3>{{ selectedProduct.title }}</h3>\r\n        <p>{{ selectedProduct.description }}</p>\r\n        <p><strong>Preço:</strong> {{ selectedProduct.price }}</p>\r\n        <p><strong>Categoria:</strong> {{ selectedProduct.category }}</p>\r\n        <p><strong>Classificação:</strong> {{ selectedProduct.rating.rate }}</p>\r\n      </div>\r\n    </Modal>\r\n\r\n    <!-- Botões para Adicionar e Remover produtos -->\r\n    <div class=\"product-actions\">\r\n      <button @click=\"toggleAddProductForm\">Adicionar Produto</button>\r\n    </div>\r\n\r\n    <!-- Formulário para adicionar novo produto -->\r\n    <div v-if=\"showAddProductForm\" class=\"add-product-form\">\r\n      <h3>Adicionar Produto</h3>\r\n      <form @submit.prevent=\"addProduct\">\r\n        <div>\r\n          <label for=\"title\">Nome</label>\r\n          <input v-model=\"newProduct.title\" type=\"text\" id=\"title\" required />\r\n        </div>\r\n        <div>\r\n          <label for=\"category\">Categoria</label>\r\n          <select v-model=\"newProduct.category\" id=\"category\" required>\r\n            <option value=\"\">Selecione uma Categoria</option>\r\n            <option value=\"electronics\">Eletrônicos</option>\r\n            <option value=\"jewelery\">Jóias</option>\r\n            <option value=\"men's clothing\">Roupas Masculinas</option>\r\n            <option value=\"women's clothing\">Roupas Femininas</option>\r\n          </select>\r\n        </div>\r\n        <div>\r\n          <label for=\"price\">Preço</label>\r\n          <input v-model=\"newProduct.price\" type=\"number\" id=\"price\" required />\r\n        </div>\r\n        <div>\r\n          <label for=\"rating\">Classificação</label>\r\n          <input v-model=\"newProduct.rating\" type=\"number\" id=\"rating\" min=\"1\" max=\"5\" required />\r\n        </div>\r\n        <button type=\"submit\">Salvar Produto</button>\r\n        <button type=\"button\" @click=\"toggleAddProductForm\">Cancelar</button>\r\n      </form>\r\n    </div>\r\n\r\n    <!-- Formulário para editar produto -->\r\n    <div v-if=\"showEditProductForm\" class=\"edit-product-form\">\r\n      <h3>Editar Produto</h3>\r\n      <form @submit.prevent=\"updateProduct\">\r\n        <div>\r\n          <label for=\"editTitle\">Nome</label>\r\n          <input v-model=\"selectedProduct.title\" type=\"text\" id=\"editTitle\" required />\r\n        </div>\r\n        <div>\r\n          <label for=\"editCategory\">Categoria</label>\r\n          <select v-model=\"selectedProduct.category\" id=\"editCategory\" required>\r\n            <option value=\"electronics\">Eletrônicos</option>\r\n            <option value=\"jewelery\">Jóias</option>\r\n            <option value=\"men's clothing\">Roupas Masculinas</option>\r\n            <option value=\"women's clothing\">Roupas Femininas</option>\r\n          </select>\r\n        </div>\r\n        <div>\r\n          <label for=\"editPrice\">Preço</label>\r\n          <input v-model=\"selectedProduct.price\" type=\"number\" id=\"editPrice\" required />\r\n        </div>\r\n        <div>\r\n          <label for=\"editRating\">Classificação</label>\r\n          <input v-model=\"selectedProduct.rating\" type=\"number\" id=\"editRating\" min=\"1\" max=\"5\" required />\r\n        </div>\r\n        <button type=\"submit\">Atualizar Produto</button>\r\n        <button type=\"button\" @click=\"cancelEdit\">Cancelar</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex';\r\nimport ProductTable from '../components/ProductTable.vue';\r\nimport Modal from '../components/Modal.vue'; // Supondo que o Modal já esteja configurado\r\n\r\nexport default {\r\n  components: {ProductTable, Modal},\r\n  data() {\r\n    return {\r\n      products: [],\r\n      filteredProducts: [],\r\n      categoryFilter: '',\r\n      sortOption: 'price',\r\n      showModal: false,\r\n      selectedProduct: null,\r\n      showAddProductForm: false, // Controle para exibir o formulário de adição de produto\r\n      showEditProductForm: false, // Controle para exibir o formulário de edição de produto\r\n      newProduct: {\r\n        title: '',\r\n        category: '',\r\n        price: null,\r\n        rating: null,\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      allProducts: (state) => state.products, // Puxando produtos do Vuex\r\n    }),\r\n  },\r\n  mounted() {\r\n    this.fetchProducts();\r\n  },\r\n  methods: {\r\n    fetchProducts() {\r\n      this.products = this.allProducts;\r\n      this.filteredProducts = this.products;\r\n    },\r\n    applyFilters() {\r\n      this.filteredProducts = this.products.filter(product => {\r\n        if (this.categoryFilter && product.category !== this.categoryFilter) {\r\n          return false;\r\n        }\r\n        return true;\r\n      });\r\n      this.applySorting(); // Aplica a ordenação após o filtro\r\n    },\r\n    applySorting() {\r\n      if (this.sortOption === 'price') {\r\n        this.filteredProducts.sort((a, b) => a.price - b.price);\r\n      } else if (this.sortOption === 'rating') {\r\n        this.filteredProducts.sort((a, b) => b.rating - a.rating);\r\n      }\r\n    },\r\n    toggleAddProductForm() {\r\n      this.showAddProductForm = !this.showAddProductForm;\r\n    },\r\n    addProduct() {\r\n      if (this.newProduct.title && this.newProduct.category && this.newProduct.price && this.newProduct.rating) {\r\n        // Adiciona o novo produto ao Vuex\r\n        this.$store.commit('addProduct', this.newProduct);\r\n\r\n        // Limpar o formulário após adicionar\r\n        this.newProduct = {\r\n          title: '',\r\n          category: '',\r\n          price: null,\r\n          rating: null,\r\n        };\r\n        this.showAddProductForm = false;\r\n      } else {\r\n        alert('Por favor, preencha todos os campos!');\r\n      }\r\n    },\r\n    deleteProduct(product) {\r\n      this.products = this.products.filter(p => p.id !== product.id);\r\n    },\r\n    editProduct(product) {\r\n      this.selectedProduct = {...product};\r\n      this.showEditProductForm = true;\r\n    },\r\n    updateProduct() {\r\n      // Atualiza o produto no Vuex\r\n      this.$store.commit('updateProduct', this.selectedProduct);\r\n\r\n      this.showEditProductForm = false;\r\n    },\r\n    cancelEdit() {\r\n      this.showEditProductForm = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.edit-product-form {\r\n  margin-top: 20px;\r\n}\r\n\r\n.edit-product-form form {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.edit-product-form label {\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.edit-product-form input,\r\n.edit-product-form select {\r\n  margin-bottom: 10px;\r\n}\r\n</style>"],"mappings":";;AA0GA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,YAAW,MAAO,gCAAgC;AACzD,OAAOC,KAAI,MAAO,yBAAyB,EAAE;;AAE7C,eAAe;EACbC,UAAU,EAAE;IAACF,YAAY;IAAEC;EAAK,CAAC;EACjCE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,EAAE;MACpBC,cAAc,EAAE,EAAE;MAClBC,UAAU,EAAE,OAAO;MACnBC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE,IAAI;MACrBC,kBAAkB,EAAE,KAAK;MAAE;MAC3BC,mBAAmB,EAAE,KAAK;MAAE;MAC5BC,UAAU,EAAE;QACVC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,IAAI;QACXC,MAAM,EAAE;MACV;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGlB,QA