{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport { mapState } from 'vuex';\nimport ProductTable from '../components/ProductTable.vue';\nimport Modal from '../components/ModalMode.vue';\nexport default {\n  components: {\n    ProductTable,\n    Modal\n  },\n  data() {\n    return {\n      categoryFilter: '',\n      sortOption: 'price',\n      showModal: false,\n      selectedProduct: null\n    };\n  },\n  computed: {\n    ...mapState({\n      products: state => state.products // Pegando produtos do Vuex\n    }),\n    filteredProducts() {\n      let filtered = [...this.products];\n\n      // Aplica filtro de categoria\n      if (this.categoryFilter) {\n        filtered = filtered.filter(product => product.category === this.categoryFilter);\n      }\n\n      // Aplica ordenação\n      if (this.sortOption === 'price') {\n        filtered.sort((a, b) => a.price - b.price);\n      } else if (this.sortOption === 'rating') {\n        filtered.sort((a, b) => b.rating.rate - a.rating.rate);\n      }\n      return filtered;\n    }\n  },\n  methods: {\n    applyFilters() {\n      // Reaplica os filtros e a ordenação automaticamente\n    },\n    applySorting() {\n      // Reaplica os filtros e a ordenação automaticamente\n    },\n    addProduct() {\n      const newProduct = {\n        id: Date.now(),\n        // ID único temporário\n        title: 'Novo Produto',\n        category: 'Eletrônicos',\n        price: 100,\n        rating: {\n          rate: 4.5\n        }\n      };\n      this.$store.dispatch('addProduct', newProduct); // Adiciona um produto\n    },\n    removeProduct() {\n      if (this.selectedProduct) {\n        this.$store.dispatch('removeProduct', this.selectedProduct.id); // Remove o produto selecionado\n        this.selectedProduct = null;\n      }\n    },\n    editProduct() {\n      if (this.selectedProduct) {\n        // Navega para página de edição ou abre modal de edição\n        this.$router.push(`/edit-product/${this.selectedProduct.id}`);\n      }\n    },\n    showProductDetails(product) {\n      this.selectedProduct = product;\n      this.showModal = true;\n    }\n  }\n};","map":{"version":3,"names":["mapState","ProductTable","Modal","components","data","categoryFilter","sortOption","showModal","selectedProduct","computed","products","state","filteredProducts","filtered","filter","product","category","sort","a","b","price","rating","rate","methods","applyFilters","applySorting","addProduct","newProduct","id","Date","now","title","$store","dispatch","removeProduct","editProduct","$router","push","showProductDetails"],"sources":["C:\\Users\\tiago\\WebstormProjects\\FakestoreS\\online-store-dashboard\\src\\views\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h2>Produtos</h2>\r\n\r\n    <!-- Filtros e Classificação -->\r\n    <div class=\"filters\">\r\n      <select v-model=\"categoryFilter\" @change=\"applyFilters\">\r\n        <option value=\"\">Selecione a Categoria</option>\r\n        <option value=\"electronics\">Eletrônicos</option>\r\n        <option value=\"jewelery\">Jóias</option>\r\n        <option value=\"men's clothing\">Roupas Masculinas</option>\r\n        <option value=\"women's clothing\">Roupas Femininas</option>\r\n      </select>\r\n\r\n      <select v-model=\"sortOption\" @change=\"applySorting\">\r\n        <option value=\"price\">Preço</option>\r\n        <option value=\"rating\">Classificação</option>\r\n      </select>\r\n    </div>\r\n\r\n    <ProductTable :products=\"filteredProducts\" @viewProduct=\"showProductDetails\" />\r\n\r\n    <!-- Modal para exibir detalhes do produto -->\r\n    <Modal v-if=\"showModal\" @close=\"showModal = false\">\r\n      <div v-if=\"selectedProduct\">\r\n        <h3>{{ selectedProduct.title }}</h3>\r\n        <p>{{ selectedProduct.description }}</p>\r\n        <p><strong>Preço:</strong> {{ selectedProduct.price }}</p>\r\n        <p><strong>Categoria:</strong> {{ selectedProduct.category }}</p>\r\n        <p><strong>Classificação:</strong> {{ selectedProduct.rating.rate }}</p>\r\n      </div>\r\n    </Modal>\r\n\r\n    <!-- Botões para Adicionar, Atualizar e Remover produtos -->\r\n    <div class=\"product-actions\">\r\n      <button @click=\"addProduct\">Adicionar Produto</button>\r\n      <button @click=\"removeProduct\" v-if=\"selectedProduct\">Remover Produto</button>\r\n      <button @click=\"editProduct\" v-if=\"selectedProduct\">Editar Produto</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapState} from 'vuex';\r\nimport ProductTable from '../components/ProductTable.vue';\r\nimport Modal from '../components/ModalMode.vue';\r\n\r\nexport default {\r\n  components: {ProductTable, Modal},\r\n  data() {\r\n    return {\r\n      categoryFilter: '',\r\n      sortOption: 'price',\r\n      showModal: false,\r\n      selectedProduct: null,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      products: (state) => state.products, // Pegando produtos do Vuex\r\n    }),\r\n    filteredProducts() {\r\n      let filtered = [...this.products];\r\n\r\n      // Aplica filtro de categoria\r\n      if (this.categoryFilter) {\r\n        filtered = filtered.filter(product => product.category === this.categoryFilter);\r\n      }\r\n\r\n      // Aplica ordenação\r\n      if (this.sortOption === 'price') {\r\n        filtered.sort((a, b) => a.price - b.price);\r\n      } else if (this.sortOption === 'rating') {\r\n        filtered.sort((a, b) => b.rating.rate - a.rating.rate);\r\n      }\r\n\r\n      return filtered;\r\n    },\r\n  },\r\n  methods: {\r\n    applyFilters() {\r\n      // Reaplica os filtros e a ordenação automaticamente\r\n    },\r\n    applySorting() {\r\n      // Reaplica os filtros e a ordenação automaticamente\r\n    },\r\n    addProduct() {\r\n      const newProduct = {\r\n        id: Date.now(), // ID único temporário\r\n        title: 'Novo Produto',\r\n        category: 'Eletrônicos',\r\n        price: 100,\r\n        rating: {rate: 4.5},\r\n      };\r\n      this.$store.dispatch('addProduct', newProduct); // Adiciona um produto\r\n    },\r\n    removeProduct() {\r\n      if (this.selectedProduct) {\r\n        this.$store.dispatch('removeProduct', this.selectedProduct.id); // Remove o produto selecionado\r\n        this.selectedProduct = null;\r\n      }\r\n    },\r\n    editProduct() {\r\n      if (this.selectedProduct) {\r\n        // Navega para página de edição ou abre modal de edição\r\n        this.$router.push(`/edit-product/${this.selectedProduct.id}`);\r\n      }\r\n    },\r\n    showProductDetails(product) {\r\n      this.selectedProduct = product;\r\n      this.showModal = true;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.filters {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.filters select {\r\n  margin-right: 10px;\r\n}\r\n\r\n.product-actions {\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"mappings":";;;AA2CA,SAAQA,QAAQ,QAAO,MAAM;AAC7B,OAAOC,YAAW,MAAO,gCAAgC;AACzD,OAAOC,KAAI,MAAO,6BAA6B;AAE/C,eAAe;EACbC,UAAU,EAAE;IAACF,YAAY;IAAEC;EAAK,CAAC;EACjCE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,EAAE;MAClBC,UAAU,EAAE,OAAO;MACnBC,SAAS,EAAE,KAAK;MAChBC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGT,QAAQ,CAAC;MACVU,QAAQ,EAAGC,KAAK,IAAKA,KAAK,CAACD,QAAQ,CAAE;IACvC,CAAC,CAAC;IACFE,gBAAgBA,CAAA,EAAG;MACjB,IAAIC,QAAO,GAAI,CAAC,GAAG,IAAI,CAACH,QAAQ,CAAC;;MAEjC;MACA,IAAI,IAAI,CAACL,cAAc,EAAE;QACvBQ,QAAO,GAAIA,QAAQ,CAACC,MAAM,CAACC,OAAM,IAAKA,OAAO,CAACC,QAAO,KAAM,IAAI,CAACX,cAAc,CAAC;MACjF;;MAEA;MACA,IAAI,IAAI,CAACC,UAAS,KAAM,OAAO,EAAE;QAC/BO,QAAQ,CAACI,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACE,KAAI,GAAID,CAAC,CAACC,KAAK,CAAC;MAC5C,OAAO,IAAI,IAAI,CAACd,UAAS,KAAM,QAAQ,EAAE;QACvCO,QAAQ,CAACI,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKA,CAAC,CAACE,MAAM,CAACC,IAAG,GAAIJ,CAAC,CAACG,MAAM,CAACC,IAAI,CAAC;MACxD;MAEA,OAAOT,QAAQ;IACjB;EACF,CAAC;EAC